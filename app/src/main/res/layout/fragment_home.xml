<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#F7FFFFFF"
    tools:context=".eshfeeny.home.HomeFragment">




    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
       >

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            >

            <ImageView
                android:id="@+id/company_id_phorto_home"
                android:layout_width="419sp"
                android:layout_height="150sp"
                android:layout_marginTop="180sp"
                android:background="#FFFFFF"
                android:backgroundTint="#FFFFFF"
                android:clickable="true"
                android:src="@drawable/insurance_company_logo_home"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.5"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                tools:ignore="SpeakableTextPresentCheck" />

            <TextView
                android:id="@+id/textView5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="16dp"
                android:fontFamily="@font/cairo_semibold"
                android:text="سهلنا الطلب عليك"
                android:textColor="#1F1F1F"
                android:textSize="14sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/company_id_phorto_home" />

            <ImageView
                android:id="@+id/add_roshta_photo_id"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/add_roshtaaa"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView5" />

            <ImageView
                android:id="@+id/SearchForMed_btn_id_home"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="5dp"
                android:clickable="true"
                android:src="@drawable/search_for_medicine_home"
                app:layout_constraintEnd_toStartOf="@+id/add_roshta_photo_id"
                app:layout_constraintTop_toBottomOf="@+id/textView5" />

            <TextView
                android:id="@+id/textView6"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="16dp"
                android:fontFamily="@font/cairo_semibold"
                android:text="تصفح الأقسام"
                android:textColor="#1F1F1F"
                android:textSize="18dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/SearchForMed_btn_id_home" />

            <ImageView
                android:id="@+id/VP_btn_id_Home"
                android:layout_width="117dp"
                android:layout_height="98dp"
                android:layout_marginTop="4dp"
                android:layout_marginEnd="8dp"
                android:clickable="true"
                android:src="@drawable/el_7maya_mn_el_versus"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView6" />

            <ImageView
                android:id="@+id/MAC_btn_id_Home"
                android:layout_width="76dp"
                android:layout_height="101dp"
                android:layout_marginTop="4dp"
                android:layout_marginEnd="44dp"
                android:clickable="true"
                android:src="@drawable/elom_w_eltfl"
                app:layout_constraintEnd_toStartOf="@+id/VP_btn_id_Home"
                app:layout_constraintTop_toBottomOf="@+id/textView6" />

            <ImageView
                android:id="@+id/WP_btn_id_Home"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="60dp"
                android:src="@drawable/montgat_el_mr2aa"
                app:layout_constraintEnd_toStartOf="@+id/MAC_btn_id_Home"
                app:layout_constraintTop_toTopOf="@+id/MAC_btn_id_Home" />

            <ImageView
                android:id="@+id/SAH_btn_id_Home"
                android:layout_width="115dp"
                android:layout_height="101dp"
                android:layout_marginTop="10dp"
                android:clickable="true"
                android:src="@drawable/el_3naya_bl_bshraa"
                app:layout_constraintEnd_toEndOf="@+id/VP_btn_id_Home"
                app:layout_constraintTop_toBottomOf="@+id/VP_btn_id_Home" />

            <ImageView
                android:id="@+id/DC_btn_id_Home"
                android:layout_width="79dp"
                android:layout_height="98dp"
                android:layout_marginEnd="44dp"
                android:src="@drawable/el_3naya_bl_asnaan"
                app:layout_constraintEnd_toStartOf="@+id/SAH_btn_id_Home"
                app:layout_constraintTop_toTopOf="@+id/SAH_btn_id_Home" />

            <ImageView
                android:id="@+id/MP_btn_id_Home"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="60dp"
                android:clickable="true"
                android:contentDescription="TODO"
                android:src="@drawable/montgat_el_rgaal"
                app:layout_constraintEnd_toStartOf="@+id/DC_btn_id_Home"
                app:layout_constraintTop_toTopOf="@+id/DC_btn_id_Home" />

            <TextView
                android:id="@+id/textView7"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="20dp"
                android:fontFamily="@font/cairo_semibold"
                android:text="احتياجات الصيف"
                android:textColor="#1F1F1F"
                android:textSize="16sp"
                app:layout_constraintEnd_toEndOf="@+id/textView6"
                app:layout_constraintTop_toBottomOf="@+id/SAH_btn_id_Home" />

            <ImageView
                android:id="@+id/select_all_summer_needs_id"
                android:layout_width="86dp"
                android:layout_height="48dp"
                android:layout_marginStart="16dp"
                android:layout_marginTop="12dp"
                android:clickable="true"
                android:src="@drawable/show_all_icon"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/SAH_btn_id_Home"
                tools:ignore="SpeakableTextPresentCheck" />

            <androidx.recyclerview.widget.RecyclerView
                android:id="@+id/medicine_id_rv"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginStart="8dp"
                android:layout_marginTop="12dp"
                android:layout_marginEnd="8dp"
                android:orientation="horizontal"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/textView7"
                app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                android:layoutDirection="rtl"
                />

            <TextView
                android:id="@+id/textView8"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="15dp"
                android:fontFamily="@font/cairo_semibold"
                android:text="لصحة أفضل"
                android:textColor="#1F1F1F"
                android:textSize="16sp"
                app:layout_constraintEnd_toEndOf="@+id/textView6"
                app:layout_constraintTop_toBottomOf="@+id/medicine_id_rv" />

            <ImageView
                android:id="@+id/select_all_for_better_health_id"
                android:layout_width="86dp"
                android:layout_height="48dp"
                android:layout_marginStart="16dp"
                android:layout_marginTop="7dp"
                android:clickable="true"
                android:src="@drawable/show_all_icon"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/medicine_id_rv"
                tools:ignore="SpeakableTextPresentCheck" />
            <androidx.recyclerview.widget.RecyclerView
                android:id="@+id/medicine_id_rv2"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginStart="8dp"
                android:layout_marginTop="12dp"
                android:layout_marginEnd="8dp"
                android:orientation="horizontal"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/textView8"
                app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                android:layoutDirection="rtl"/>
            <TextView
                android:id="@+id/textView9"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="15dp"
                android:fontFamily="@font/cairo_semibold"
                android:text="بديل للسكر"
                android:textColor="#1F1F1F"
                android:textSize="16sp"
                app:layout_constraintEnd_toEndOf="@+id/textView6"
                app:layout_constraintTop_toBottomOf="@+id/medicine_id_rv2" />

            <ImageView
                android:id="@+id/select_all_alternative_to_sugar_id"
                android:layout_width="86dp"
                android:layout_height="48dp"
                android:layout_marginStart="16dp"
                android:layout_marginTop="7dp"
                android:clickable="true"
                android:src="@drawable/show_all_icon"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/medicine_id_rv2"
                tools:ignore="SpeakableTextPresentCheck" />
            <androidx.recyclerview.widget.RecyclerView
                android:id="@+id/medicine_id_rv3"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginStart="8dp"
                android:layout_marginTop="12dp"
                android:layout_marginEnd="8dp"
                android:orientation="horizontal"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/textView9"
                app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                android:layoutDirection="rtl"/>
            <TextView
                android:id="@+id/textView10"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="15dp"
                android:fontFamily="@font/cairo_semibold"
                android:text = "تصفح الماركات"
                android:textColor="#1F1F1F"
                android:textSize="16sp"
                app:layout_constraintEnd_toEndOf="@+id/textView6"
                app:layout_constraintTop_toBottomOf="@+id/medicine_id_rv3" />

            <ImageView
                android:id="@+id/select_all_Markat_id"
                android:layout_width="86dp"
                android:layout_height="48dp"
                android:layout_marginStart="16dp"
                android:layout_marginTop="7dp"
                android:clickable="true"
                android:src="@drawable/show_all_icon"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/medicine_id_rv3"
                tools:ignore="SpeakableTextPresentCheck" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginTop="8dp"
                android:orientation="vertical"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/textView10">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:orientation="horizontal">

                    <com.google.android.material.card.MaterialCardView
                        android:id="@+id/beesline_btn_id"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_margin="8dp"
                        android:backgroundTint="#F7FFFFFF"
                        android:elevation="0dp"
                        android:layoutDirection="rtl"
                        android:padding="6dp"
                        app:cardCornerRadius="10sp"
                        app:strokeColor="#e5e5e5"
                        app:strokeWidth="0.4sp"
                        android:clickable="true">

                        <ImageView
                            android:layout_width="99sp"
                            android:layout_height="73sp"
                            android:layout_margin="6sp"
                            android:src="@drawable/vichy_laboratories_logo_vector"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent" />
                    </com.google.android.material.card.MaterialCardView>

                    <com.google.android.material.card.MaterialCardView
                        android:id="@+id/vichy_btn_id"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_margin="8dp"
                        android:backgroundTint="#F7FFFFFF"
                        android:elevation="0dp"
                        android:layoutDirection="rtl"
                        android:padding="6dp"
                        app:cardCornerRadius="10sp"
                        app:strokeColor="#e5e5e5"
                        app:strokeWidth="0.4sp"
                        android:clickable="true">

                        <ImageView
                            android:layout_width="99sp"
                            android:layout_height="73sp"
                            android:layout_margin="6sp"
                            android:src="@drawable/vichy_laboratories_logo_vector"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent" />
                    </com.google.android.material.card.MaterialCardView>

                    <com.google.android.material.card.MaterialCardView
                        android:id="@+id/laRoche_btn_id"
                        android:clickable="true"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_margin="8dp"
                        android:backgroundTint="#F7FFFFFF"
                        android:elevation="0dp"
                        android:layoutDirection="rtl"
                        android:padding="6dp"
                        app:cardCornerRadius="10sp"
                        app:strokeColor="#e5e5e5"
                        app:strokeWidth="0.4sp">

                        <ImageView
                            android:layout_width="99sp"
                            android:layout_height="73sp"
                            android:layout_margin="6sp"
                            android:src="@drawable/la_roche_posay_brand"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent" />
                    </com.google.android.material.card.MaterialCardView>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:orientation="horizontal">

                    <com.google.android.material.card.MaterialCardView
                        android:id="@+id/johnsons_btn_id"
                        android:clickable="true"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_margin="8dp"
                        android:backgroundTint="#F7FFFFFF"
                        android:elevation="0dp"
                        android:layoutDirection="rtl"
                        android:padding="6dp"
                        app:cardCornerRadius="10sp"
                        app:strokeColor="#e5e5e5"
                        app:strokeWidth="0.4sp">

                        <ImageView
                            android:layout_width="99sp"
                            android:layout_height="73sp"
                            android:layout_margin="6sp"
                            android:src="@drawable/pngwing_com"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent" />
                    </com.google.android.material.card.MaterialCardView>

                    <com.google.android.material.card.MaterialCardView
                        android:id="@+id/garnier_btn_id"
                        android:clickable="true"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_margin="8dp"
                        android:backgroundTint="#F7FFFFFF"
                        android:elevation="0dp"
                        android:layoutDirection="rtl"
                        android:padding="6dp"
                        app:cardCornerRadius="10sp"
                        app:strokeColor="#e5e5e5"
                        app:strokeWidth="0.4sp">

                        <ImageView
                            android:layout_width="99sp"
                            android:layout_height="73sp"
                            android:layout_margin="6sp"
                            android:src="@drawable/garnier_logo"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent" />
                    </com.google.android.material.card.MaterialCardView>

                    <com.google.android.material.card.MaterialCardView
                        android:id="@+id/Axe_btn_id"
                        android:clickable="true"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_margin="8dp"
                        android:backgroundTint="#F7FFFFFF"
                        android:elevation="0dp"
                        android:layoutDirection="rtl"
                        android:padding="6dp"
                        app:cardCornerRadius="10sp"
                        app:strokeColor="#e5e5e5"
                        app:strokeWidth="0.4sp">

                        <ImageView
                            android:layout_width="99sp"
                            android:layout_height="73sp"
                            android:layout_margin="6sp"
                            android:src="@drawable/axe_logo_2021"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent" />
                    </com.google.android.material.card.MaterialCardView>
                </LinearLayout>

            </LinearLayout>
            <View
                android:id="@+id/view"
                android:layout_width="wrap_content"
                android:layout_height="420sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@id/textView10" />

        </androidx.constraintlayout.widget.ConstraintLayout>
    </ScrollView>


</androidx.constraintlayout.widget.ConstraintLayout>