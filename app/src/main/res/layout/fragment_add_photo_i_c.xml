<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    android:backgroundTint="#FFFFFF"
    tools:context=".eshfeeny.insurance_card.AddPhotoICFragment">

    <ImageView
        android:id="@+id/addPhotoICid"
        android:layout_width="match_parent"
        android:layout_height="150sp"
        android:layout_marginHorizontal="16sp"
        android:layout_marginTop="16dp"
        android:clickable="true"
        android:maxHeight="250sp"
        android:minWidth="200sp"
        android:minHeight="200sp"
        android:visibility="visible"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView17"
        app:srcCompat="@drawable/cardic2" />

    <TextView
        android:id="@+id/tVaddInfoCard"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:fontFamily="@font/cairo_semibold"
        android:text="بيانات الكارت"
        android:textColor="#1F1F1F"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="HardcodedText" />

    <ImageView
        android:id="@+id/backBtn22"
        android:layout_width="27sp"
        android:layout_height="40sp"
        android:layout_gravity="center_vertical|right"
        android:layout_marginTop="13dp"
        android:layout_marginEnd="16sp"
        android:clickable="true"
        android:contentDescription="@string/todo"
        android:focusable="true"
        android:src="@drawable/exit_logo"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="ContentDescription,RtlHardcoded,SpeakableTextPresentCheck,TouchTargetSizeCheck,DuplicateSpeakableTextCheck" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="80sp"
        android:layout_height="80sp"
        android:layout_marginTop="16dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tVaddInfoCard" />

    <Button
        android:id="@+id/add_CartExists_Btn"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="16sp"
        android:backgroundTint="#E5E5E5"
        android:fontFamily="@font/cairo_medium"
        android:padding="8sp"
        android:text="حفظ الكارت"
        android:textColor="#676767"
        android:textSize="18sp"
        android:visibility="visible"
        app:cornerRadius="10sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView17"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="32dp"
        android:layout_marginEnd="16dp"
        android:fontFamily="@font/cairo_medium"
        android:text="صورة الكارت"
        android:textColor="#1F1F1F"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView4" />

    <com.google.android.material.button.MaterialButton
        android:id="@+id/ReUploadCardId"
        style="@style/Widget.MaterialComponents.Button.OutlinedButton.Icon"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="إعادة رفع صورة الكارت "
        android:textColor="#0583F2"
        android:textSize="18sp"
        android:visibility="gone"
        app:cornerRadius="10sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/addPhotoICid"
        app:rippleColor="#0583F2"
        app:strokeColor="#0583F2" />
</androidx.constraintlayout.widget.ConstraintLayout>